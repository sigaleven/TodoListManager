//		//process bar start
//		//get data from parse
		ParseQuery<ParseObject> query = ParseQuery.getQuery("todo");
//		query.whereEqualTo("title", "User");
//		query.whereEqualTo("userId", deviceId);
		query.findInBackground(new FindCallback<ParseObject>() {
			public void done(List<ParseObject> items, ParseException e) {
				if (e != null) {
					Log.d("todo", "error");
				} else {
					if(items!=null){
						//process bar end
						for(ParseObject item : items){
							String[] arr = new String[2];
							arr[0] = item.getString("title");
							arr[1] = item.getString("due");
							toDoList.add(arr);
							adapter.notifyDataSetChanged();
						}
					}else{
						Log.d("todo", "new user");
					}
				}
			}
		});
		
		
		
				ParseUser.enableAutomaticUser();
		//		Log.d("todo", "enableUser");
		ParseUser.getCurrentUser().saveInBackground();
		TelephonyManager mngr = (TelephonyManager)getSystemService(Context.TELEPHONY_SERVICE); 
		deviceId = mngr.getDeviceId();
		
		
		
					//updateParse ADD
			ParseObject todo = new ParseObject("todo");
			todo.put("title", arr[0]);
			todo.put("due", arr[1]);
			todo.put("user",ParseUser.getCurrentUser());
			todo.put("userId", deviceId);
			todo.setACL(new ParseACL(ParseUser.getCurrentUser()));
			todo.saveInBackground();